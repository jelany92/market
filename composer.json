{
  "name": "yiisoft/yii2-app-advanced",
  "description": "Yii 2 Advanced Project Template",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "project template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2.0",
    "yiisoft/yii2": "~2.0.39.2",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
    "philippfrenzel/yii2fullcalendar": "~3.9.0",
    "2amigos/yii2-tinymce-widget": "~1.1",
    "kartik-v/yii2-widget-datepicker": "dev-master",
    "kartik-v/yii2-widget-datetimepicker": "*",
    "yiisoft/yii2-bootstrap4": "~2.0.6",
    "kartik-v/yii2-detail-view": "@dev",
    "kartik-v/yii2-widget-activeform": "dev-master",
    "sjaakp/yii2-sortable-behavior": "*",
    "yiisoft/yii2-imagine": "*",
    "kartik-v/yii2-icons": "@dev",
    "kartik-v/yii2-widget-select2": "dev-master",
    "kartik-v/yii2-widget-fileinput": "@dev",
    "kartik-v/yii2-widget-sidenav": "*",
    "kartik-v/yii2-export": "@dev",
    "kartik-v/yii2-mpdf": "*",
    "kartik-v/yii2-grid": "@dev",
    "yii2tech/spreadsheet": "*",
    "2amigos/yii2-translateable-behavior": "~1.1",
    "codemix/yii2-localeurls": "^1.7",
    "onmotion/yii2-widget-apexcharts": "^1.0",
    "coderius/yii2-hit-counter": "1.0.2.2",
    "kartik-v/yii2-editable": "dev-master",
    "kartik-v/yii2-social": "@dev",
    "yiisoft/yii2-authclient": "~2.1.0",
    "kartik-v/bootstrap-popover-x": "dev-master",
    "yidas/yii2-fontawesome": "^5.0",
    "kartik-v/yii2-helpers": "dev-master",
    "kartik-v/yii2-widget-depdrop": "dev-master",
    "wbraganca/yii2-dynamicform": "dev-master",
    "bower-asset/font-awesome": "5.5.*",
    "kartik-v/yii2-widget-colorinput": "*",
    "evgeniyrru/yii2-slick": "*",
    "newerton/yii2-fancybox": "dev-master",
    "yiisoft/yii2-queue": "^2.3"

  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.1.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/codeception": "4.0.x-dev | ^4.0",
    "codeception/module-asserts": "^1.0",
    "codeception/module-yii2": "^1.1",
    "codeception/module-filesystem": "^1.0",
    "phpunit/phpunit": "~5.7.27 || ~6.5.5",
    "codeception/verify": "~0.5.0 || ~1.1.0",
    "symfony/browser-kit": ">=2.7 <=4.2.4",
    "2amigos/yii2-highcharts-widget": "~1.0",
    "symfony/var-dumper": "^5.0",
    "codeception/module-webdriver": "^1.1"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
